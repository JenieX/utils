  "scripts": {
    "build": "rollup --config",
    "develop": "npm run build -- --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@jeniex/eslint-config": "^0.0.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.5.7",
    "@violentmonkey/types": "^0.1.5",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "tslib": "^2.6.2"
  },
  "publishConfig": {
    "access": "public"
